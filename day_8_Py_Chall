def main():
    # 1. Create an empty dictionary called dog
    dog = {}
    # 2. Add name, color, breed, legs, age to the dog dictionary
    dog = {'Name': 'Teo', 'Color': 'Yellow', 'Breed': 'Golden,', 'Legs': 4, 'Age': 1}
    # 3. Create a student dictionary and add first_name, last_name, gender, age, marital status, skills, country,
    # city and address as keys for the dictionary
    student = {'first_name': 'Camilo', 'last_name': 'Rojas', 'gender': 'male', 'age': 26, 'martial_status': 'single',
               'skills': ['Programming', 'logistics'], 'country': 'Colombia'}

    # 4. Get the length of the student dictionary
    print(len(student))
    # 5. Get the value of skills and check the data type, it should be a list
    print(type(student['skills']))
    # 6. Modify the skills values by adding one or two skills
    student['skills'] = ['Programming', 'logistics', 'Quality control ']
    # 7. Get the dictionary keys as a list
    keys = student.keys()
    print(keys)
    # 8. Get the dictionary values as a list
    values = student.values()
    print(values)
    # 9. Change the dictionary to a list of tuples using items() method
    items = student.items()
    print(items)
    # 10. Delete one of the items in the dictionary
    student.pop('skills')
    # 11. Delete one of the dictionaries
    del student


main()
